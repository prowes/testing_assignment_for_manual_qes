[Unit]
Description=Gunicorn instance to serve an a Flask app
After=network.target

[Service]
User=ec2-user
WorkingDirectory=/home/ec2-user/flapp/testing_assignment_for_manual_qes/
Environment="/home/ec2-user/flapp/venv/bin"
ExecStart=/home/ec2-user/flapp/venv/bin/gunicorn --workers 3 --bind unix:myproject.sock -m 007 application:application

[Install]
WantedBy=multi-user.target